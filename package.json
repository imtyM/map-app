    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-sass": "^4.9.2",
    "prismjs": "^1.15.0",
    "react": "^16.3.1",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-md": "^1.2.9",
    "react-share": "^2.1.0",
    "recompose": "^0.30.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
